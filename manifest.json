{
    "name": "JSON Visualizer",
    "version": "1.0.0.0",
    "description": "This extension dispalys a JSON string in a reader friendly layout.",
    "author": "Mridul Sethia",
	"homepage_url": "https://github.com/mridulsethia/My.Extensions.Edge.JsonVisualizer",
    "content_scripts": [ {
      "matches": ["*://*/*"],
      "css": ["css/content.css"],
      "js": ["js/jquery.js","js/content.js"],
      "run_at": "document_end"
    } ],
	"permissions": [ "*://*/*" ],
	"icons": {
       "32": "icons/json_32.png"
    },
    "minimum_edge_version": "38.14393.0.0",
	"web_accessible_resources" : ["icons/*.png"]
}
